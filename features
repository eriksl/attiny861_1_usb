MODEL 6 (ATTINY861A), VERSION 1, REVISION 0

*** COMMANDS ***

	byte	command (see below)
	byte*	arguments

	bit		7	6	5	4	3		2	1	0


			0	0	0	0	0						0x00	command 0
			...
			1	1	1	1	1						0xf8	command 31

									0	0	0		0x00	i/o 0
									...
									1	1	1		0x07	i/o 7

			0	0	0	0	0		0	0	0		0x00	identify, returns: 0x00 0x00 0x4a 0xfb AA BB CC {DD} where
																	AA[4] = numeric device id
																	BB[5] = firmware version
																	CC[6] = firmware revision
																	DD[7] = ascii id string
			0	0	0	0	0		0	0	1		0x01	read adc, returns 2 bytes
			0	0	0	0	0		0	1	0		0x02	timer0 counter, returns 1 byte
			0	0	0	0	0		0	1	1		0x03	timer1 counter, returns 2 bytes
			0	0	0	0	0		1	1	1		0x07	extended command, rest follows after this byte

			0	0	0	1	0						0x10	read counter, returns 4 bytes
			0	0	1	0	0						0x20	read/reset counter, returns like read counter

			0	0	1	1	0						0x30	read input, returns 1 byte

			0	1	0	0	0						0x40	write output / softpwm, 1 byte, 0 = off, 255 = on, other is pwm
			0	1	0	1	0						0x50	read output / softpwm, 1 byte like write output / softpwm
			0	1	1	0	0						0x60	write softpwm mode, supply 1 byte
			0	1	1	1	0						0x70	read softpwm mode, returns 1 byte

			1	0	0	0	0						0x80	write pwm, 2 bytes, 0 = off, > 1023 = on
			1	0	0	1	0						0x90	read pwm, 2 bytes like write pwm
			1	0	1	0	0						0xa0	write hardpwm mode, supply 1 byte
			1	0	1	1	0						0xb0	read hardpwm mode, returns 1 byte

			1	1	0	0	0						0xc0	start adc conversion, analog input #

	softpwm mode =
			0	on
			1	fade in
			2	fade out
			3	fade in/out

	EXTENDED COMMANDS

			0x00	get digital inputs		returns 1 byte amount, 4 bytes maximum counter value (0 = no counter)
			0x01	get analog inputs		returns 1 byte amount, 2 bytes mininum value, 2 bytes maximum value
			0x02	get digital outputs		returns 1 byte amount, 2 bytes maximum pwm value (0 = no pwm)
			0x03	get pwm outputs			returns 1 byte amount, 2 bytes maximum pwm value

*** REPLY ***

	byte	length of reply minus this length byte
	byte	error code
	byte	command this reply relates to
	byte*	reply information
	byte	checksum (skip length byte)

	error codes =
			0	ok
			1	no command
			2	unknown command
			3	parameter out of range
			4	missing parameter(s)
			5	not ready
			6	internal error
			7	not implemented

*** FEATURES ***

	pin		name		usb		pin function	connection

	1		pb0					MOSI/PB0		output 1
	2		pb1					MISO/OC1A		output pwm 1
	3		pb2					SCK/PB2			output 2
	4		pb3					OC1B			output pwm 2
	5		Vcc			V+		VCC				X
	6		GND			V-		GND				X
	7		pb4					XTAL1			X
	8		pb5					XTAL2			X
	9		pb6					PCINT14/PB6		input 1
	10		pb7					RESET			X
	11		pa7					ADC6/AIN1		adc input 1
	12		pa6					ADC5/AIN0		adc input 2
	13		pa5					PCINT5/PA5		input 2
	14		pa4					PCINT4/PA4		input 3
	15		AVcc				AVCC			X
	16		AGND				AGND			X
	17		pa3					PA3				input sense led
	18		pa2			D-		INT1			USB d-
	19		pa1					PA1				USB d+
	20		pa0			D+		PA0				command sense led
